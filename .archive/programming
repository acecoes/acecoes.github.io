// This is the function we declared.
// What information can we get from the header?
int isaprime (int num) {

    // Variable declaration
    // What's the difference between declaration and initialization?
    int i;

    for (i = 2; i <= sqrt(num); i++) {
            if (num __ i == 0) {
                    return 0;
            }
            else {
                    ;
            }
    }

    return true;
}

